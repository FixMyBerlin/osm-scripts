{
  "scripts": {
    "lint": "eslint --fix --ext .ts .",
    "prettier": "prettier --write .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:prepare:download": "npx ts-node ./ZESPlus/Highways-PrepareData/1-download-highways/download.ts",
    "build:prepare:transpose": "npx ts-node ./ZESPlus/Highways-PrepareData/2-transpose-highways/transpose",
    "build:run:BicycleWayData": "npx ts-node ./ZESPlus/Highways-BicycleWayData/process.ts",
    "build:run:SurfaceData": "npx ts-node ./ZESPlus/Highways-SurfaceData/process.ts",
    "build:run:HighwayTypeData": "npx ts-node ./ZESPlus/Highways-HighwayTypeData/process.ts",
    "build": "npm run build:prepare:download && npm run build:prepare:transpose && npm run build:run:BicycleWayData && npm run build:run:SurfaceData && npm run build:run:HighwayTypeData"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "node-fetch": "^3.1.0",
    "osmtogeojson": "^3.0.0-beta.4",
    "prettier": "2.4.1",
    "typescript": "^4.5.2"
