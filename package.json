{
  "scripts": {
    "lint": "eslint --fix --ext .ts .",
    "prettier": "prettier --write .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "data:build:downloadArea": "npx ts-node ./ZESPlus/AreaOfInterest/download.ts && npx ts-node ./ZESPlus/AreaOfInterest/transpose.ts",
    "data:build:download": "npx ts-node ./ZESPlus/Highways-PrepareData/1-download-highways/download.ts",
    "data:build:transpose": "npx ts-node ./ZESPlus/Highways-PrepareData/2-transpose-highways/transpose",
    "data:build:BicycleWayData": "npx ts-node ./ZESPlus/Highways-BicycleWayData/process.ts",
    "data:build:SurfaceData": "npx ts-node ./ZESPlus/Highways-SurfaceData/process.ts",
    "data:build:HighwayTypeData": "npx ts-node ./ZESPlus/Highways-HighwayTypeData/process.ts",
    "data:build": "npm run data:build:downloadArea && npm run data:build:download && npm run data:build:transpose && npm run data:build:BicycleWayData && npm run data:build:SurfaceData && npm run data:build:HighwayTypeData",
    "11ty:build": "npm run 11ty:build:11ty && npm run 11ty:build:tailwind",
    "11ty:build:11ty": "cd StatusBoard && npx @11ty/eleventy",
    "11ty:build:tailwind": "cd StatusBoard && NODE_ENV=production npx tailwindcss -i css/tailwind.input.css -o css/tailwind.output.css",
    "11ty:start:11ty": "cd StatusBoard && npx @11ty/eleventy --serve",
    "11ty:start:tailwind": "cd StatusBoard && npx tailwindcss -i css/tailwind.input.css -o css/tailwind.output.css --watch",
    "11ty:start": "npm-run-all --parallel 11ty:start:*",
    "build": "npm run data:build #&& 11ty:build # <- 11ty is not running, yet",
    "build:netlify": "SKIP_LEFTOVER_CHECK=true npm run build",
    "start": "npm run 11ty:start"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^16.11.7",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "node-fetch": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "osmtogeojson": "^3.0.0-beta.4",
    "prettier": "2.4.1",
    "tailwindcss": "^3.0.11",
    "typescript": "^4.5.2",
    "de9im": "^1.3.1"
  }
}
